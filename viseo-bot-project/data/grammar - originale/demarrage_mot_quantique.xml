<grammar version="1.0" xml:lang="fr-FR" mode="voice" root="start_mot_quantique" xmlns="http://www.w3.org/2001/06/grammar" tag-format="semantics/1.0">
	<rule id="start_mot_quantique" scope="public">
		<tag>out.action=new Object(); </tag>
		<item>SWAN</item>
		<one-of>
			<item>démarre le moteur quantique</item>
			<item>allume le moteur quantique</item>
			<item>démarre le quantum</item>
			<item>allume le quantum</item>
			<item>lance le moteur quantique</item>
			<item>active le moteur quantique</item>
			<item>met en route le moteur quantique</item>
			<item>calcul un saut quantique</item>
			<item>calcul un saut</item>
		</one-of>
		<tag>out.action.action="quantum_on";</tag>
	</rule> 
</grammar>