<grammar version="1.0" xml:lang="fr-FR" mode="voice" root="ouvrir_porte" xmlns="http://www.w3.org/2001/06/grammar" tag-format="semantics/1.0">
	<rule id="ouvrir_porte" scope="public">
		<tag>out.action=new Object(); </tag>
		<item>SWAN</item>
		<!--Ouvertures des issues-->
		<one-of>
			<item>ouvre la porte</item>
			<item>ouvre les portes</item>
			<item>ouvre le sasse</item>
			<item>ouvre les sasse</item>
			<item>ouvre la soute</item>
			<item>ouvre les soutes</item>
		</one-of>
		<tag>out.action.action="open_door";</tag>
	</rule> 
</grammar>