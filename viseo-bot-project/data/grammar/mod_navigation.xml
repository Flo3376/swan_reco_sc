<grammar version="1.0" xml:lang="fr-FR" mode="voice" root="switch_to_navigation" xmlns="http://www.w3.org/2001/06/grammar" tag-format="semantics/1.0">
	<rule id="switch_to_navigation" scope="public">
		<tag>out.action=new Object(); </tag>
		<item>SWAN</item>
		<one-of>
			<item>passe en mode navigation</item> <!--triange de puissance au centre, bouclier équilibré -->
			<item>mode navigation</item> <!--triange de puissance au centre, bouclier équilibré -->
		</one-of>
		<tag>out.action.action="switch_to_navigation";</tag>
	</rule> 
</grammar>