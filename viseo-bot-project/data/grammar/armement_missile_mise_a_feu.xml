<grammar version="1.0" xml:lang="fr-FR" mode="voice" root="fire_missile" xmlns="http://www.w3.org/2001/06/grammar" tag-format="semantics/1.0">
	<rule id="fire_missile" scope="public">
		<tag>out.action=new Object(); </tag>
		<item>SWAN</item>
		<!--Armement lancer les missiles-->
		<one-of>
			<item>feu à volonté</item>
			<item>mise à feu</item>
			<item>mise à feu des missiles</item>
			<item>lance le missile</item>
			<item>lance les missile</item>
		</one-of>
		<tag>out.action.action="fire_missile";</tag>
	</rule> 
</grammar>