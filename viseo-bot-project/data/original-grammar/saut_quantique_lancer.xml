<grammar version="1.0" xml:lang="fr-FR" mode="voice" root="lancer_saut_quantique" xmlns="http://www.w3.org/2001/06/grammar" tag-format="semantics/1.0">
	<rule id="lancer_saut_quantique" scope="public">
		<tag>out.action=new Object(); </tag>
		<item>SWAN</item>
		<!--Lancer un saut quantique-->
		<one-of>
			<item>Lance le saut</item>
			<item>On saute</item>
			<item>go pour le saut</item>
			<item>Hyperespace</item>
		</one-of>
		<tag>out.action.action="launch_jump_q";</tag>
	</rule> 
</grammar>