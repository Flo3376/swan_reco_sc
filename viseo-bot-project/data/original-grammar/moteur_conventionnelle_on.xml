<grammar version="1.0" xml:lang="fr-FR" mode="voice" root="start_engine" xmlns="http://www.w3.org/2001/06/grammar" tag-format="semantics/1.0">
	<rule id="start_engine" scope="public">
		<tag>out.action=new Object(); </tag>
		<item>SWAN</item>
		<!--Démarre les moteur convetionels -->
		<one-of>
			<item>démarre le moteur</item>
			<item>démarre les moteurs</item>
			<item>allume les moteurs</item>
			<item>lance le moteur</item>
			<item>lance les moteurs</item>
			<item>fait chauffer les moteurs</item>
			<item>fait chauffer le moteur</item>
		</one-of>
		<tag>out.action.action="start_engine";</tag>
	</rule> 
</grammar>