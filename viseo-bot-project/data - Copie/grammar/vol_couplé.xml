<grammar version="1.0" xml:lang="fr-FR" mode="voice" root="coupled_off" xmlns="http://www.w3.org/2001/06/grammar" tag-format="semantics/1.0">
	<rule id="coupled_off" scope="public">
		<tag>out.action=new Object(); </tag>
		<item>SWAN</item>
		<!--ouverture ou passage en VTOL-->
		<one-of>
			
			<item>découple</item>
			<item>découplage</item>
			<item>découplage moteur</item>
		</one-of>
		<tag>out.action.action="coupled_off";</tag>
	</rule> 
</grammar>