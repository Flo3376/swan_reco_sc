<grammar version="1.0" xml:lang="fr-FR" mode="voice" root="coupled_on" xmlns="http://www.w3.org/2001/06/grammar" tag-format="semantics/1.0">
	<rule id="coupled_on" scope="public">
		<tag>out.action=new Object(); </tag>
		<item>SWAN</item>
		<!--ouverture ou passage en VTOL-->
		<one-of>
			<item>recouple</item>
			<item>recouplage</item>
			<item>recouplage moteur</item>
		</one-of>
		<tag>out.action.action="coupled_on";</tag>
	</rule> 
</grammar>